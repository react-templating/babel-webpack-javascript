    "@svgr/webpack": "5.5.0",
    "babel-loader": "8.2.2",
    "clean-webpack-plugin": "3.0.0",
    "compression-webpack-plugin": "7.1.2",
    "copy-webpack-plugin": "8.1.1",
    "cross-env": "7.0.3",
    "css-loader": "5.2.4",
    "file-loader": "6.2.0",
