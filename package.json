{
  "name": "babel-webpack-javascript",
  "version": "1.0.0",
  "dependencies": {
    "dotenv": "8.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "sass": "1.32.11",
    "web-vitals": "1.1.1"
  },
  "scripts": {
    "dev": "webpack serve --config webpack/webpack.dev.js",
    "build": "webpack --config webpack/webpack.prod.js",
    "eject": "react-scripts eject",
    "heroku-postbuild": "webpack --config webpack/webpack.prod.js",
